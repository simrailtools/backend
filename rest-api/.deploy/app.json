{
  "name": "api",
  "description": "Public API for SIT",
  "repository": "https://github.com/simrailtools/backend",
  "addons": [],
  "healthchecks": {
    "web": [
      {
        "wait": 3,
        "attempts": 20,
        "initialDelay": 5,
        "type": "startup",
        "path": "/admin/health",
        "name": "container startup check"
      },
      {
        "wait": 5,
        "type": "liveness",
        "path": "/admin/health",
        "name": "container liveness check"
      }
    ]
  }
}
